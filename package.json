  "scripts": {
    "build": "rollup -c",
    "build:docs": "npm run render:readme && typedoc && sed -i.bak \"/\\*\\*@cori-risi/d\" ./docs/README.md && sed -i.bak '/\\*\\*@cori-risi/d' ./docs/**.md && sed -i.bak '/\\*\\*@cori-risi/d' ./docs/**/*.md && sed -i.bak 's/Function\\: //g' ./docs/**.md && sed -i.bak 's/Function\\: //g' ./docs/**/*.md && sed -i.bak 's/Variable\\: //g' ./docs/**.md && sed -i.bak 's/Variable\\: //g' ./docs/**/*.md",
    "build:r": "Rscript -e 'pkgbuild::clean_dll(); pkgbuild::compile_dll(); devtools::document(); devtools::check(); devtools::install();'",
    "tsc": "tsc --jsx react --resolveJsonModule",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npm run build:docs && npm run render:docs && python -m http.server -d _site 3000",
    "render:readme": "quarto render R/README.qmd --to gfm --output README.md",
    "render:docs": "quarto render",
    "type:check": "tsc --noEmit",
    "test": "jest --no-cache",
    "test:clean": "jest --clearCache",
    "view:docs": "npm run render:docs && python -m http.server -d _site 3000"
  }
}
